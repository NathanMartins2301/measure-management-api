
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Reading{
  id                Int @id @default(autoincrement())
  image_url         String
  measure_value     Int
  measure_uuid      String @unique
  customer_code     String 
  measure_datetime  DateTime
  measure_type      String
  has_confirmed     Boolean @default(false)
}
